;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="We Wizards 3"
config/description="A co-op game where you play a team of wizards fighting off evil invaders (the tax colllector)"
run/main_scene="res://Scenes/Menus/MainMenu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0.133333, 0.121569, 0.211765, 1)
boot_splash/image="res://Sprites/first_pancake_standin.png"
config/icon="res://Sprites/Characters/Warlock/Warlock.svg"

[autoload]

GameManager="*res://Scripts/GameManager.gd"
SoundManager="*res://Scripts/SoundManager.gd"

[debug]

gdscript/warnings/untyped_declaration=1

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"

[file_customization]

folder_colors={
"res://Cards/": "blue",
"res://Cards/Card Sprites/": "yellow",
"res://Enemies/": "red",
"res://Enemies/Enemy Sprites/": "yellow",
"res://Levels/": "pink",
"res://Projectiles/Projectile Sounds/": "green",
"res://SaveData/": "blue",
"res://Sprites/": "yellow",
"res://Storeroom/Footstep Sounds/": "green"
}

